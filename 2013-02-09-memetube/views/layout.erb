<!doctype html>
<html lang='en'>
  <head>
    <meta charset='utf-8'>

  <link rel='stylesheet' href='/bootstrap/css/bootstrap.css'>

    <script src='/js/jquery-1.9.1.js'></script>
    <script src='/bootstrap/js/bootstrap.js'></script>
     <link rel='stylesheet' href='/css/style.css'>



    <title>memetube</title>
  </head>
  <body>

    <div class="container">
      <div class="row">
        <div class="span12">

          <div class="navbar navbar-inverse">
            <div class="navbar-inner">
              <a class="brand" href="/">Stasher</a>
              <ul class="nav">
                <li><a href="/new">New</a></li>
                <li><a href="/vids">vids</a></li>
                <% @nav_rows.each do |row| %>

                  <% if row['genre'].length > 0 %>
                  <li><a href='/vids/<%= row['genre'] %>'><%= row['genre'] %></a></li>
                  <% end %>
              <% end %>

              </ul>
            </div>
          </div>

        </div>
      </div>









    <%= yield %>
  </body>
</html>

